{
  "manifest_version": 2,

  "name": "Dimmer",
  "description": "dimmer",
  "version": "0.1",
  "permissions": [
    "storage"
  ],
  "background": {
    "scripts" : ["jquery-3.0.0.min.js", "background.js"]
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["jquery-3.0.0.min.js", "main.js"],
      "css": ["main.css"]
    }
  ]
}
